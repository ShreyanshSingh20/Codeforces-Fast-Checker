<body class="text-center">

  <div class="form-signin">
    <form action="/" method="POST">
      <img class="mb-4" src="logo.png" alt="" width="150" height="150">
      <h1 class="h3 mb-3 fw-normal">Please Enter The Details</h1>

      <div class="form-floating">
        <input type="text" class="form-control" id="floatingInput" placeholder="Handle" name=handle>
        <label for="floatingInput">Handle</label>
      </div>
      <div class="form-floating">
        <input type="text" class="form-control" id="floatingPassword" placeholder="Contest ID" name="id">
        <label for="floatingPassword">Contest ID</label>
      </div>


      <button class="w-100 btn btn-lg btn-primary" type="submit">GO!</button>
    </form>
  </div>
